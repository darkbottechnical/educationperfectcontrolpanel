              in Science, English, Maths, History and Geography. Learn 11+ foreign languages including French, Japanese, Chinese
              (Mandarin), German, Spanish, Italian, Indonesian, MÄori and Hebrew.">
        <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no,viewport-fit=cover">
        <meta name="robots" content="noindex">
        <script>
            var IS_NATIVE_MOBILE = !1
              , userAgent = navigator.userAgent
              , isSearchEngine = /googlebot/i.test(userAgent) || /bingbot/i.test(userAgent)
              , isPdfGeneration = /pdf/i.test(userAgent) || /phantomjs/i.test(userAgent)
              , BROWSER_REGEX = "Edge?\\/(79|[89]\\d|\\d{3,})\\.\\d+(\\.\\d+|)|Firefox\\/(6[7-9]|[7-9]\\d|\\d{3,})\\.\\d+(\\.\\d+|)|Chrom(ium|e)\\/(7[5-9]|[89]\\d|\\d{3,})\\.\\d+(\\.\\d+|)|(Maci|X1{2}).+ Version\\/(12\\.\\d+|(1[3-9]|[2-9]\\d|\\d{3,})\\.\\d+)([,.]\\d+|)( \\(\\w+\\)|)( Mobile\\/\\w+|) Safari\\/|Chrome.+OPR\\/(10[5-9]|1[1-9]\\d|[2-9]\\d{2}|\\d{4,})\\.\\d+\\.\\d+|(CPU[ +]OS|iPhone[ +]OS|CPU[ +]iPhone|CPU IPhone OS|CPU iPad OS)[ +]+(12[._]\\d+|(1[3-9]|[2-9]\\d|\\d{3,})[._]\\d+)([._]\\d+|)|Opera Mini|Android:?[ /-](12[1-9]|1[3-9]\\d|[2-9]\\d{2}|\\d{4,})(\\.\\d+|)(\\.\\d+|)|Mobile Safari.+OPR\\/(7[3-9]|[89]\\d|\\d{3,})\\.\\d+\\.\\d+|Android.+Firefox\\/(12[2-9]|1[3-9]\\d|[2-9]\\d{2}|\\d{4,})\\.\\d+(\\.\\d+|)|Android.+Chrom(ium|e)\\/(12[1-9]|1[3-9]\\d|[2-9]\\d{2}|\\d{4,})\\.\\d+(\\.\\d+|)|Android.+(UC? ?Browser|UCWEB|U3)[ /]?(15\\.([5-9]|\\d{2,})|(1[6-9]|[2-9]\\d|\\d{3,})\\.\\d+)\\.\\d+|SamsungBrowser\\/(2[2-9]|[3-9]\\d|\\d{3,})\\.\\d+|Android.+MQ{2}Browser\\/(13(\\.([1-9]|\\d{2,})|)|(1[4-9]|[2-9]\\d|\\d{3,})(\\.\\d+|))(\\.\\d+|)|K[Aa][Ii]OS\\/(2\\.([5-9]|\\d{2,})|([3-9]|\\d{2,})\\.\\d+)(\\.\\d+|)"
              , SAFARI_13_REGEX = "(Maci|X1{2}).+ Version\\/(13\\.\\d+|(1[4-9]|[2-9]\\d|\\d{3,})\\.\\d+)([,.]\\d+|)( \\(\\w+\\)|)( Mobile\\/\\w+|) Safari\\/"
              , SAFARI_COMMA_VERSION_REGEX = /(Version\/\d+\.\d+)\,(\d+ Safari\/)/;
            SAFARI_COMMA_VERSION_REGEX.test(userAgent) && (userAgent = userAgent.replace(SAFARI_COMMA_VERSION_REGEX, "$1.$2"));
            var isSupportedBrowser = new RegExp(BROWSER_REGEX).test(userAgent) || new RegExp(SAFARI_13_REGEX).test(userAgent) && userAgent.includes("Mac") && navigator.maxTouchPoints >= 1;
            isSupportedBrowser || isSearchEngine || isPdfGeneration || IS_NATIVE_MOBILE || (window.location.href = "https://update-browser.educationperfect.com")
        </script>
        <script>
            const BUILD_NAME = "main"
              , VANITY_HOSTNAMES = ["app.educationperfect.com", "app.ca.educationperfect.com"]
              , {hostname: hostname, origin: origin, pathname: pathname} = window.location
              , isVanityUrl = VANITY_HOSTNAMES.includes(window.location.hostname)
              , buildName = isVanityUrl ? "." : "main"
              , baseUrl = new URL(`${buildName}/`,origin);
            baseUrl.buildName = buildName;
            const baseElement = document.createElement("base");
            baseElement.setAttribute("href", IS_NATIVE_MOBILE ? "/" : baseUrl.href),
            document.head.insertBefore(baseElement, document.head.firstChild),
            window.__MFE_BASE_URL__ = Object.freeze(baseUrl)
        </script>
